{
  "name": "spesql",
  "version": "0.1.0",
  "description": "Database management application for the FLMNH",
  "author": {
    "name": "Aaron Leopold"
  },
  "build": {
    "appId": "spesql",
    "win": {
      "icon": "build/flmnhLogo.png"
    }
  },
  "proxy": "http://localhost:5000",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "build:electron": "electron-builder",
    "build:react": "react-scripts build",
    "build": "yarn build:react && yarn build:electron",
    "run:dev": "nodemon public/server/server.js",
    "run:prod": "node public/server/server.js",
    "start:browser": "concurrently \"yarn run:dev\"  \"react-scripts start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cross-env": "^6.0.3",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "mocha": "^6.2.3",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^1.19.4",
    "papaparse": "^5.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-checkmark": "^1.1.2",
    "react-csv": "^1.1.2",
    "react-datepicker": "^2.15.0",
    "react-dom": "^16.11.0",
    "react-dropzone": "^11.0.1",
    "react-infinite-scroll-component": "^5.0.4",
    "react-notifications": "^1.5.0",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scroll": "^1.7.16",
    "react-semantic-ui-datepickers": "^2.4.0",
    "react-sortable-hoc": "^1.11.0",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "should": "^13.2.3",
    "styled-components": "^5.0.1",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^7.1.0",
    "electron-builder": "^22.1.0",
    "wait-on": "^3.3.0"
  }
}
