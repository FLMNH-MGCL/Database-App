{
  "name": "spesql",
  "version": "1.0.0",
  "author": "Aaron Leopold",
  "description": "MySQL portal",
  "main": "startelectron.js",
  "scripts": {
    "dev-build": "rm -rf dist && cross-env NODE_ENV=development npm run dev-server-build && npm run dev-client-build",
    "dev-server-build": "webpack --mode development --config configs/webpack.config.server.js",
    "dev-client-build": "webpack --mode development --config configs/webpack.config.dev.js",
    "prod-build": "rm -rf dist && cross-env NODE_ENV=production npm run prod-server-build && npm run prod-client-build",
    "prod-server-build": "webpack --mode production --config configs/webpack.config.server.js",
    "prod-client-build": "webpack --mode production --config configs/webpack.config.prod.js",
    "dev": "npm run dev-build && electron .",
    "package": "npm run prod-build && electron-packager ./ --out=electron-build --overwrite"
  },
  "repository": "https://github.com/FLMNH-MGCL/spesql",
  "keywords": [
    "react",
    "electron",
    "express",
    "webpack",
    "boilerplate"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mocha": "^8.1.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "papaparse": "^5.2.0",
    "react": "^16.7.0",
    "react-checkmark": "^1.3.0",
    "react-csv": "^2.0.3",
    "react-datepicker": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.3",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.1",
    "react-semantic-ui-datepickers": "^2.9.0",
    "react-sortable-hoc": "^1.11.0",
    "react-virtualized": "^9.22.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "regenerator-runtime": "^0.13.7",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.2.0",
    "should": "^13.2.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^2.1.0",
    "electron": "^4.0.1",
    "electron-packager": "^13.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.1",
    "styled-components": "^5.1.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  }
}
